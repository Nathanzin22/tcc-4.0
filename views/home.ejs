<div id="carousel" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-indicators" class="carousel-indicators">
        <button type="button" data-bs-target="#carousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <div class="carousel-inner">
        <% if(user) { %>
            <div class="carousel-item active" id="carousel-4">
                <div class="carousele">
                    <div class="titulo">
                        <h1>PORTAL DO PROFISSIONAL</h1>
                        <form role="search" method="post" class="d-flex" action="/busca">
                            <select name="profissao" id="profissao" class="form-control">
                                <% if(tag != null) { %>
                                    <% tag.forEach((tag)=>{ %>
                                        <option value="<%= tag.profissao %>"><%= tag.profissao %></option>
                                    <% }) %>
                                <% } else { %>
                                    <option value="">Sem Agenda</option>
                                <% } %>
                            </select>
                            <button class="btn btn-success" type="submit">Search</button>
                        </form>
                    </div>
                </div>
            </div>
        <% }else{ %>
            <div class="carousel-item active" id="carousel-1">
                <div class="carousele">
                    <div class="titulo">
                        <h1>cabeleireiros excelentes!</h1>
                        <p> Descubra e reserve profissionais de beleza perto de ti</p>
                        <form role="search" method="post" class="d-flex" action="/busca">
                            <select name="profissao" id="profissao" class="form-control">
                                <% if(tag.length > 0) { %>
                                    <% tag.forEach((tag)=>{ %>
                                        <option value="<%= tag.profissao %>"><%= tag.profissao %></option>
                                    <% }) %>
                                <% } else { %>
                                    <option value="">Sem Agenda</option>
                                <% } %>
                            </select>
                            <button class="btn btn-success" type="submit">Search</button>
                        </form>
                    </div>
                </div>
            </div>
            <div class="carousel-item " id="carousel-2">
                <div class="carousele">
                    <div class="titulo">
                        <h1>Encontre e faça agendamento</h1>
                        <p>Descubra e reserve profissionais de beleza perto de ti</p>
                        <form role="search" method="post" class="d-flex" action="/busca">
                            <select name="profissao" id="profissao" class="form-control">
                                <% if(tag.length > 0 ) { %>
                                    <% tag.forEach((tag)=>{ %>
                                        <option value="<%= tag.profissao %>"><%= tag.profissao %></option>
                                    <% }) %>
                                <% } else { %>
                                    <option value="0">Sem Agenda</option>
                                <% } %>
                            </select>
                            <button class="btn btn-success" type="submit">Search</button>
                        </form>
                    </div>
                </div>
            </div>
            <div class="carousel-item" id="carousel-3">
                <div class="carousele">
                    <div class="titulo">
                        <h1>Simplifique Já</h1>
                        <p>Com o uso de um sistema online, você pode automatizar esse processo, economizando seu
                            trabalho manual ou da sua equipe.</p>
                        <form role="search" method="post" class="d-flex" action="/busca">
                            <select name="profissao" id="profissao" class="form-control">
                                <% if(tag.length > 0) { %>
                                    <% tag.forEach((tag)=>{ %>
                                        <option value="<%= tag.profissao %>"><%= tag.profissao %></option>
                                    <% }) %>
                                <% } else { %>
                                    <option value="">Sem Agenda</option>
                                <% } %>
                            </select>
                            <button class="btn btn-success" type="submit">Search</button>
                        </form>
                    </div>
                </div>
            </div>
        <% } %>
    </div>
</div>
<br>
<!-- Button trigger modal -->
<% if(profissao != null) { %>
    <div class="container">
        <br>
        <h3>Resultado da Pesquisa</h3>
        <hr>
        <div class="row justify-content-sm-center">
            <% if(profissao.length > 0) { %>
                <% profissao.forEach((profissao) => {
                if (profissao.disponibilidade) {  // Exibe apenas se disponibilidade for true
                %>
                <div class="col-sm-6 col-md-6">
                    <div class="card mb-5">
                        <div class="card-body">
                            <!-- Exibe o status da agenda -->
                            <div style="display: flex; justify-content: space-between">
                                <h4>Agenda <strong class="alert-success">Aberta</strong></h4>

                                <button class="btn btn-dark mt-1" data-bs-target="#servicoModal" data-bs-toggle="modal">
                                    Serviços
                                </button>

                                <div class="modal fade" id="servicoModal" tabindex="-1" aria-labelledby="servicoModalNome" aria-hidden="true">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="servicoModalNome">Serviços</h5>
                                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                            </div>
                                            <div class="modal-body">
                                                <p><%= profissao.servico %></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <% if(profissao.horario > '17:59:00') { %>
                                <p>Disponibilidade: <strong class="alert-primary">Noite</strong></p>
                            <% } else if(profissao.horario > '11:59:00') { %>
                                <p>Disponibilidade: <strong class="alert-primary">Tarde</strong></p>
                            <% } else { %>
                                <p>Disponibilidade: <strong class="alert-primary">Manhã</strong></p>
                            <% } %>

                            <!-- Formatação da data no padrão DD/MM/YYYY -->
                            <small>Data:
                                <strong><%= new Date(profissao.data).toLocaleDateString('pt-BR') %></strong></small>
                            <br>

                            <!-- Formatação do horário no padrão HH:mm -->
                            <small>Horário: <strong><%= profissao.horario.substring(0, 5) %></strong></small>

                            <br>
                            <a class="btn btn-outline-success"
                               href="/reservar/<%= profissao.id %>/<%= profissao.user_id %>">Reservar</a>
                        </div>
                    </div>
                </div>
                <% } // Fechamento do if disponibilidade   %>
                <% }) %>
            <% } else { %>
                <p>Nenhuma agenda disponível</p>
            <% } %>
        </div>
    </div>
    <!-- Modal -->
<% } else {
} %>
